# DailyOwo Environment Variables Example
# Copy this file to .env.local and fill in your values

# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=

# Email Service (Resend)
RESEND_API_KEY=

# AI Configuration
# Get your Gemini API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=

# Optional: OpenAI Configuration
# OPENAI_API_KEY=

# Optional: Anthropic Configuration
# ANTHROPIC_API_KEY=

# AI Feature Flags
NEXT_PUBLIC_AI_RECEIPT_ENABLED=true
NEXT_PUBLIC_AI_INSIGHTS_ENABLED=true
NEXT_PUBLIC_AI_BUDGET_ENABLED=false
NEXT_PUBLIC_AI_CHAT_ENABLED=false

# AI Scanning Mode
# Set to 'false' to use OCR-first approach (traditional)
# Set to 'true' or leave unset to use AI-first approach (recommended)
NEXT_PUBLIC_AI_FIRST_SCANNING=true 